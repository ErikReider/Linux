{
  "coc.preferences.bracketEnterImprove": true,
  "coc.preferences.colorSupport": true,
  "clangd.semanticHighlighting": true,
  "clangd.arguments": ["-fallback-style=Google"],
  "clangd.checkUpdates": true,
  "languageserver": {
    "digestif": {
      "command": "digestif",
      "filetypes": ["tex", "plaintex", "context"]
    },
    "vala": {
      "command": "vala-language-server",
      "filetypes": ["vala", "genie"]
    }
  },
  "Lua.workspace.library": {
    "/usr/share/nvim/runtime/lua": true,
    "/usr/share/nvim/runtime": true
  },
  "Lua.runtime.version": "LuaJIT",
  "Lua.telemetry.enable": false,
  "Lua.workspace.maxPreload": 2000,
  "Lua.workspace.preloadFileSize": 1000,
  "Lua.diagnostics.enable": true,
  "Lua.diagnostics.globals": [
    "hs",
    "vim",
    "it",
    "describe",
    "before_each",
    "after_each"
  ],
  "Lua.diagnostics.disable": ["lowercase-global"],
  "lua.checkForUpdates": "ask",
  "cSpell.language": "en,sv",
  "cSpell.dictionaries": ["swedish"],
  "html-css-support.enabledLanguages": [
    "html",
    "vue",
    "blade",
    "htmldjango",
    "typescriptreact",
    "javascriptreact"
  ],
  "tsserver.enableJavascript": true,
  "javascript.showUnused": true,
  "javascript.validate.enable": true,
  "javascript.suggest.autoImports": true,
  "eslint.autoFixOnSave": true,
  "diagnostic.enableSign": true,
  "diagnostic.errorSign": "❌",
  "diagnostic.warningSign": "⚠️",
  "diagnostic.hintSign": "?",
  "diagnostic.infoSign": "",
  "emmet.showExpandedAbbreviation": "always",
  "emmet.showAbbreviationSuggestions": true,
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["typescript"],
    "typescriptreact": ["typescript"]
  },
  "suggest.completionItemKindLabels": {
    "method": "  ",
    "function": "  ",
    "variable": "[]",
    "field": "  ",
    "typeParameter": "<>",
    "constant": "  ",
    "class": " פּ ",
    "interface": " 蘒",
    "struct": "  ",
    "event": "  ",
    "operator": "  ",
    "module": "  ",
    "property": "  ",
    "enum": " 練",
    "reference": "  ",
    "keyword": "  ",
    "file": "  ",
    "folder": " ﱮ ",
    "color": "  ",
    "unit": " 塞 ",
    "snippet": "  ",
    "text": "  ",
    "constructor": "  ",
    "value": "  ",
    "enumMember": "  "
  }
}
