{
  "coc.preferences.bracketEnterImprove": true,
  "coc.preferences.colorSupport": true,
  "clangd.semanticHighlighting": true,
  "clangd.arguments": ["-fallback-style=Google"],
  "clangd.checkUpdates": true,
  "languageserver": {
    "digestif": {
      "command": "digestif",
      "filetypes": ["tex", "plaintex", "context"]
    },
    "vala": {
      "command": "vala-language-server",
      "filetypes": ["vala", "genie"]
    },
    "efm": {
      "command": "efm-langserver",
      "args": ["-c", "~/.config/efm-langserver/config.yaml"],
      "filetypes": ["vim", "nvim", "eruby", "markdown"]
    }
  },
  "tsserver.enableJavascript": true,
  "javascript.showUnused": true,
  "javascript.validate.enable": true,
  "javascript.suggest.autoImports": true,
  "eslint.autoFixOnSave": true,
  "diagnostic.enableSign": true,
  "diagnostic.errorSign": "❌",
  "diagnostic.warningSign": "⚠️",
  "diagnostic.hintSign": "?",
  "diagnostic.infoSign": "",
  "emmet.showExpandedAbbreviation": "always",
  "emmet.showAbbreviationSuggestions": true,
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["typescript"],
    "typescriptreact": ["typescript"]
  },
  "suggest.completionItemKindLabels": {
    "method": "  ",
    "function": "  ",
    "variable": "[]",
    "field": "  ",
    "typeParameter": "<>",
    "constant": "  ",
    "class": " פּ ",
    "interface": " 蘒",
    "struct": "  ",
    "event": "  ",
    "operator": "  ",
    "module": "  ",
    "property": "  ",
    "enum": " 練",
    "reference": "  ",
    "keyword": "  ",
    "file": "  ",
    "folder": " ﱮ ",
    "color": "  ",
    "unit": " 塞 ",
    "snippet": "  ",
    "text": "  ",
    "constructor": "  ",
    "value": "  ",
    "enumMember": "  "
  }
}
